extends ../layout

block main
  h1 #{title}: #{group.name}
  p= group.description

  p Do you really want to delete this group?
  
  form(method='POST' action='')
    div.form-group
      input#groupid.form-control(type='hidden',name='id', required='true', value=group._id )
    button.btn.btn-primary(type='submit') Delete